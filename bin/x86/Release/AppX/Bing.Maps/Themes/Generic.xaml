                                        <ColorAnimation Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="selection" To="{StaticResource BackgroundStrokeColorAerialBE}" Duration="0"/>
                                        <ColorAnimation Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="background" To="{StaticResource BackgroundFillColorAerialBE}" Duration="0"/>
                                        <ColorAnimation Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="background" To="{StaticResource BackgroundStrokeColorAerialBE}" Duration="0"/>
