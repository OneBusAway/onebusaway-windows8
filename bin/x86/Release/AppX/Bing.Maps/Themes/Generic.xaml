    <Color x:Key="HoverColorAerialBE">#FF4D4D4D</Color>
    <Color x:Key="HoverIconColorAerialBE">#FFFFFFFF</Color>
    
    <Color x:Key="BackgroundFillColorRoad">#FFFFFFFF</Color>
    <Color x:Key="BackgroundStrokeColorRoad">#FFCCCCCC</Color>
    <Color x:Key="ForegroundFillColorRoad">#FF333333</Color>

