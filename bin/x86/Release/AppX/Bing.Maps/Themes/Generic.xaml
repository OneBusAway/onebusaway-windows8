    <SolidColorBrush x:Key="InfoboxBodyTextColorBrushRoad" Color="{StaticResource InfoboxBodyTextColorRoad}"/>
    <SolidColorBrush x:Key="PanelBackgroundColorBrushRoad" Color="{StaticResource PanelBackgroundColorRoad}"/>
    
    <SolidColorBrush x:Key="InfoboxPointerColorBrushRoad" Color="{StaticResource InfoboxPointerColorRoad}"/>
    <SolidColorBrush x:Key="InfoboxPointerColorBrushAerialBE" Color="{StaticResource InfoboxPointerColorAerialBE}"/>
    <SolidColorBrush x:Key="BackgroundFillColorBrushAerialBE" Color="{StaticResource BackgroundFillColorAerialBE}"/>
    <SolidColorBrush x:Key="BackgroundStrokeColorBrushAerialBE" Color="{StaticResource BackgroundStrokeColorAerialBE}"/>
    <SolidColorBrush x:Key="BackgroundFillColorBrushRoad" Color="{StaticResource BackgroundFillColorRoad}"/>
    <SolidColorBrush x:Key="BackgroundStrokeColorBrushRoad" Color="{StaticResource BackgroundStrokeColorRoad}"/>
    <SolidColorBrush x:Key="ForegroundFillColorBrushRoad" Color="{StaticResource ForegroundFillColorRoad}"/>
    <SolidColorBrush x:Key="PushColorBrushRoad" Color="{StaticResource PushColorRoad}"/>

    <SolidColorBrush x:Key="DefaultPushpinBackgroundBrush" Color="#FF4498FE"/>
    <SolidColorBrush x:Key="DefaultPushpinForegroundBrush" Color="#FFF7F4F2"/>
    
    <x:Double x:Key="ControlHeight">40</x:Double>
    <x:Double x:Key="MapTypeMenuItemSize">40</x:Double>
    
    <x:Double x:Key="LogoHeightEN">29</x:Double>
    <x:Double x:Key="LogoHeightZH">30</x:Double>
    <x:Double x:Key="CombinedLogoWidthEN">126</x:Double>
    <x:Double x:Key="CombinedLogoWidthZH">152</x:Double>
    <x:Double x:Key="BingOnlyLogoWidthEN">63</x:Double>
    <x:Double x:Key="BingOnlyLogoWidthZH">76</x:Double>
