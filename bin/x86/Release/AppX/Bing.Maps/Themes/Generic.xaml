    <SolidColorBrush x:Key="FloorControlBackgroundColorBrush" Color="{StaticResource FloorControlBackgroundColor}"/>
    <SolidColorBrush x:Key="FloorControlStrokeColorBrush" Color="{StaticResource FloorControlStrokeColor}"/>
 
