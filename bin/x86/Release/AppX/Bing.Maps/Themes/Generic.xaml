    <Color x:Key="InfoboxPointerColorRoad">#FF2F2F2F</Color>
    <Color x:Key="InfoboxPointerColorAerialBE">White</Color>
    
    <Color x:Key="FloorControlTextColor">White</Color>
    <Color x:Key="FloorControlBackgroundColor">#FF20AFFF</Color>
    <Color x:Key="FloorControlStrokeColor">#FFF1EEEE</Color>
    
