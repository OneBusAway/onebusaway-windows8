    <SolidColorBrush x:Key="InfoboxBodyTextColorBrushAerialBE" Color="{StaticResource InfoboxBodyTextColorAerialBE}"/>
    <SolidColorBrush x:Key="PanelBackgroundColorBrushAerialBE" Color="{StaticResource PanelBackgroundColorAerialBE}"/>
