                                Grid.Column="1"  
                                Margin="10,-4,0,0" 
                                Text="&#xE0A1;"
                                FontFamily="Segoe UI Symbol" 
                                FontSize="18"
                                Foreground="{StaticResource ForegroundFillColorBrushRoad}"
                                VerticalAlignment="Center"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="VenueFloorControlTemplate">
        <Grid x:Name="LayoutRoot">
            <Grid.Resources>
                <SolidColorBrush x:Name="BorderColorBrush" x:Key="BorderColorBrush" Color="{StaticResource BackgroundStrokeColorRoad}"/>
            </Grid.Resources>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="MapTypeStates">
                    <VisualState x:Name="Road">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="TextColorBrush" To="{StaticResource BackgroundStrokeColorRoad}" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="AerialBE">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="Color" Storyboard.TargetName="TextColorBrush" To="{StaticResource BackgroundStrokeColorAerialBE}" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <bm:AccessibleContainer 
                    x:Name="rootButton" 
                    UseLayoutRounding="True"
                    Style="{StaticResource FloorControlRootStyle}"/>
            </Grid>
            <Grid 
                x:Name="menuContainer"  
                Grid.Row="1" 
                Margin="0,-1,0,0"
                Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Border
                    x:Name="BackgroundBorder"
                    BorderThickness="1"
                    BorderBrush="{StaticResource BorderColorBrush}"/>
                <ItemsControl
                    x:Name="menuLight" 
                    Margin="1"
                    UseLayoutRounding="True">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <bm:AccessibleContainer Grid.Column="0" Style="{StaticResource MenuItemStyle}"/>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                            </Grid>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </Grid>
            
        </Grid>
    </ControlTemplate>

    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="VenueDirectoryListItemHeaderTextTemplate">
        <Grid x:Name="LayoutRoot" HorizontalAlignment="Stretch" VerticalAlignment="Top">
            <TextBlock x:Name="textblock" Text="{Binding Name}" Height="23" Margin="2,0,0,0" VerticalAlignment="Top" Foreground="#FF20AFFF" FontSize="16" FontFamily="Segoe UI" />
        </Grid>
    </ControlTemplate>

    <DataTemplate x:Key="VenueEntityListViewItemTemplate" >
        <Grid x:Name="LayoutRoot"  HorizontalAlignment="Stretch">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="2*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <TextBlock Text="{Binding Name}" Margin="5,0,0,0" Grid.Row="1" Grid.Column="0" x:Name="businessNameTextBlock" Foreground="Black" FontSize="14" FontFamily="Segoe UI Semilight" LineHeight="22" TextWrapping="Wrap" TextTrimming="WordEllipsis" VerticalAlignment="Center" />
            <TextBlock Text="{Binding FloorName}" Grid.Row="1" Grid.Column="1"  Margin="5,0,5,0" x:Name="levelInformationTextBlock" Foreground="Black" FontSize="14" FontFamily="Segoe UI Semilight" LineHeight="32"  TextWrapping="Wrap" TextTrimming="WordEllipsis" VerticalAlignment="Center"/>
        </Grid>
    </DataTemplate>


    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="CloseButtonTemplate">
        <Grid x:Name="LayoutRoot" HorizontalAlignment="Right" >
            <Path Data="M6.0819402,3.8478775 L9.9920959,7.7580338 L13.909088,3.8478775 L15.570221,5.5021744 L11.653229,9.4191666 L15.570221,13.336159 L13.909088,14.990456 L9.9920959,11.080299 L6.0819402,14.990456 L4.4276433,13.329323 L8.3377991,9.4191666 L4.4276433,5.5090103 z" Fill="#FF5689C7" HorizontalAlignment="Right" Height="11.142" Margin="0,8,10.858,0" RenderTransformOrigin="0.5,0.5" Stretch="Fill" VerticalAlignment="Top" Width="11.142"/>
        </Grid>
    </ControlTemplate>
    
    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="VenueDirectoryTitleTextBlockTemplate">
        <Grid x:Name="LayoutRoot" UseLayoutRounding="false">
            <TextBlock VerticalAlignment="Top" LineHeight="24" Foreground="Black" FontSize="24" TextWrapping="Wrap" FontFamily="Segoe UI Semilight" Text="{TemplateBinding Text}"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="VenueDirectoryLocationTextBlockTemplate">
        <Grid x:Name="LayoutRoot" UseLayoutRounding="false">
            <TextBlock VerticalAlignment="Top" Foreground="#FF333333" LineHeight="22" FontSize="14" FontFamily="Segoe UI Semilight" TextWrapping="Wrap" Text="{TemplateBinding Text}"/>
        </Grid>
    </ControlTemplate>

    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="VenueDirectoryCountTextBlockTemplate">
        <Grid x:Name="LayoutRoot" UseLayoutRounding="false">
            <TextBlock VerticalAlignment="Top" TextWrapping="Wrap" Foreground="Black" FontSize="18" FontFamily="Segoe UI" HorizontalAlignment="Left" Text="{TemplateBinding Text}"/>
        </Grid>
    </ControlTemplate>

    <Style TargetType="vm:VenueDirectoryPanel">
        <Setter Property="MinHeight" Value="300"/>
        <Setter Property="MinWidth" Value="275" />
        <Setter  Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="vm:VenueDirectoryPanel">
                    <Grid x:Name="rootGrid" Background="#FFF1EEEE">
                        <Grid.Resources>
                            <CollectionViewSource x:Name="venueEntitiesList" />
                        </Grid.Resources>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Rectangle Fill="White" Grid.Row="0" Stroke="#FFF1EEEE" VerticalAlignment="Stretch" Margin="0,1.334,0,0"/>
                        <Grid  Grid.Row="0" Margin="20,7,0,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                            <bm:AccessibleContainer Grid.Row="0" Grid.Column="0" x:Name="titleTextBlock" AcceptPrimaryAction="False"  Template="{StaticResource VenueDirectoryTitleTextBlockTemplate}"  />
                            <bm:AccessibleContainer Grid.Row="0" Grid.Column="1" x:Name="closeButton" Margin="0,1,10.858,0" Template="{StaticResource CloseButtonTemplate}" />
                            <bm:AccessibleContainer Grid.Row="1" Grid.Column="0" x:Name="addressTextBlock" Margin="0,5,0,0" AcceptPrimaryAction="False" HorizontalAlignment="Left"  Template="{StaticResource VenueDirectoryLocationTextBlockTemplate}" VerticalAlignment="Top"/>
                            <bm:AccessibleContainer Grid.Row="2" Grid.Column="0" x:Name="phoneNumberTextBlock" AcceptPrimaryAction="False"  HorizontalAlignment="Left" Template="{StaticResource VenueDirectoryLocationTextBlockTemplate}"/>
                            <Rectangle Fill="Transparent" Grid.Row="3" Grid.Column="0" Height="15"/>
                        </Grid>
                         <Grid  x:Name="bodyGrid" Margin="0, 10, 0, 0" Grid.Row="1" Grid.Column="0">
                                    <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                             <bm:AccessibleContainer x:Name="directoryLabel" Grid.Row="0" Margin="20,0,0,0" AcceptPrimaryAction="False" Grid.Column="0" HorizontalAlignment="Left"  Template="{StaticResource VenueDirectoryCountTextBlockTemplate}"/>
                             <Grid Grid.Row="1" VerticalAlignment="Top" x:Name="listViewGrid" Margin="15,10,0,0"  Width="{TemplateBinding Width}">
                                 <ListView x:Name="venueEntityListView" ItemsSource="{Binding Source={StaticResource venueEntitiesList}}"  
                                              VerticalAlignment="Top" HorizontalAlignment="Stretch" ItemTemplate="{StaticResource VenueEntityListViewItemTemplate}">
                                        <ListView.ItemContainerStyle>
                                            <Style TargetType="ListViewItem">
                                                <Setter Property="Margin" Value="0,-5,5,-5"/>
                                                <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                                                <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                            </Style>
                                        </ListView.ItemContainerStyle>
                                     <ListView.GroupStyle>
                                         <GroupStyle>
                                             <GroupStyle.HeaderTemplate>
                                                 <DataTemplate x:Key="VenueEntityListViewHeaderTemplate" >
                                                     <bm:AccessibleContainer x:Name="itemHeader" Margin="5, 0, 0, 15" Text="{Binding Name}" Template="{StaticResource VenueDirectoryListItemHeaderTextTemplate}"/>
                                                 </DataTemplate>
                                             </GroupStyle.HeaderTemplate>
                                         </GroupStyle>
                                     </ListView.GroupStyle>
                                 </ListView>
                                </Grid>
                            </Grid>
                        </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!-- MAP LAYOUT -->
    <ControlTemplate TargetType="bm:Map" x:Key="MapTemplate">
        <Grid x:Name="mapRoot">
            <Grid x:Name="mapPanel" Background="Blue">
                <bm:MapLayer x:Name="internalUILayer">
                    <bm:MapLayer.Children>
                        <bm:AccessibleContainer 
                            x:Name="popup" 
                            Visibility="Collapsed"
                            Template="{StaticResource PopupTemplate}"/>
                        <bm:AccessibleContainer 
                            x:Name="infobox"  
                            Visibility="Collapsed"
                            Template="{StaticResource InfoboxTemplate}"/>
                    </bm:MapLayer.Children>
                </bm:MapLayer>
            </Grid>
            <Grid x:Name="mapForeground">
                <StackPanel 
                    x:Name="bottomLeftContainer" 
                    Margin="0,0,0,10"
                    Orientation="Vertical"
                    VerticalAlignment="Bottom" 
                    HorizontalAlignment="Left">
                    <bm:AccessibleContainer 
                        x:Name="logoElement" 
                        IsHitTestVisible="False" 
                        IsTabStop="False" 
                        AcceptPrimaryAction="False"
                        Template="{StaticResource LogoElementTemplate}"/>
                </StackPanel>
                <StackPanel
                    x:Name="bottomRightContainer" 
                    Margin="0,0,0,10"
                    Orientation="Vertical"
                    VerticalAlignment="Bottom"
                    HorizontalAlignment="Right">
                    <bm:AccessibleContainer 
                        x:Name="scalebarElement" 
                        Margin="10,10,10,0"
                        IsHitTestVisible="False" 
                        IsTabStop="False" 
                        AcceptPrimaryAction="False"
                        Template="{StaticResource ScalebarTemplate}"/>
                    <bm:AccessibleContainer 
                        x:Name="copyrightElement" 
                        Margin="10,10,10,0"
                        IsHitTestVisible="False"  
                        IsTabStop="False" 
                        AcceptPrimaryAction="False"
                        Template="{StaticResource CopyrightTemplate}"/>
                </StackPanel>
                <StackPanel 
                    x:Name="topLeftContainer" 
                    Orientation="Vertical"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Left"/>
                <StackPanel 
                    x:Name="topRightContainer" 
                    Orientation="Vertical"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Right"/>
                <Grid
                    x:Name="navControlContainer" 
                    VerticalAlignment="Top">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <bm:AccessibleContainer  
                        x:Name="breadcrumb"
                        Grid.Row="0"
                        Height="{StaticResource ControlHeight}" 
                        IsTabStop="False" 
                        AcceptPrimaryAction="False" 
                        Template="{StaticResource BreadcrumbTemplate}"
                        HorizontalAlignment="Stretch"/>
                    <bm:AccessibleContainer  
                        x:Name="navigationControl" 
                        Grid.Row="1"
                        Margin="10,10,10,0" 
                        IsTabStop="False" 
                        AcceptPrimaryAction="False" 
                        Template="{StaticResource NavigationControlTemplate}" 
                        HorizontalAlignment="Stretch"/>
                    <Grid  
                        Grid.Row="1" 
                        Margin="0,10,0,0" 
                        VerticalAlignment="Top"
                        HorizontalAlignment="Center">
                        <bm:AccessibleContainer 
                            x:Name="floorControl" 
                            Visibility="Collapsed"  
                            IsTabStop="False" 
                            AcceptPrimaryAction="False" 
                            Template="{StaticResource VenueFloorControlTemplate}"/>
                    </Grid>
                 </Grid>
            </Grid>
            <Grid x:Name="messagePanel">
                <bm:AccessibleContainer 
                    x:Name="invalidCredentialMessage" 
                    Visibility="Collapsed" 
                    Template="{StaticResource InvalidCredentialsMessageTemplate}"/>
            </Grid>
        </Grid>
    </ControlTemplate>
</ResourceDictionary>
