    <x:Int32 x:Key="MediumCopyrightFontSize">9</x:Int32>
    <x:Int32 x:Key="LargeCopyrightFontSize">10</x:Int32>
    <x:Double x:Key="DefaultPushpinSize">22</x:Double>
    <x:Double x:Key="InfoboxWidth">280</x:Double>
    
    <!-- FONTS -->
    <Style x:Key="BasicTextMedium" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="FontSize" Value="14.66"/>
        <Setter Property="LineHeight" Value="20"/>
        <Setter Property="Typography.StylisticSet20" Value="True"/>
        <Setter Property="Typography.DiscretionaryLigatures" Value="True"/>
    </Style>

    <Style x:Key="MediumText-R-None" TargetType="TextBlock" BasedOn="{StaticResource BasicTextMedium}">
        <Setter Property="FontWeight" Value="Normal"/>
    </Style>

    <Style x:Key="CompassText" TargetType="TextBlock" BasedOn="{StaticResource BasicTextMedium}">
        <Setter Property="Foreground" Value="{StaticResource ForegroundFillColorAerialBE}"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>

    <Style x:Key="BreadcrumbArrowTextBlockStyle" TargetType="TextBlock">
        <Setter Property="Text" Value="&#xE013;"/>
        <Setter Property="FontFamily" Value="Segoe UI Symbol"/>
        <Setter Property="FontSize" Value="13"/>
        <Setter Property="Foreground" Value="Transparent"/>
    </Style>

    <Style TargetType="bm:MapItemsControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <bm:MapLayer>
                        <ItemsPresenter x:Name="itemsPresenter"/>
                    </bm:MapLayer>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <bm:MapLayer/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!-- COPYRIGHT -->
    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="CopyrightTemplate">
        <Grid 
            x:Name="LayoutRoot" 
            UseLayoutRounding="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="MapTypeStates">
                    <VisualState x:Name="Road">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="copyrightShadow" To="White" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="copyright" To="Black" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="AerialBE">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="copyrightShadow" To="Black" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="copyright" To="White" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <!-- TODO (v-markfo): Investigate conflict with Scale text size -->
                <VisualStateGroup x:Name="MapSizeStates">
                    <VisualState x:Name="Small">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyright">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource SmallCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyrightShadow">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource SmallCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Medium">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyright">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource MediumCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyrightShadow">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource MediumCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Large">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyright">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource LargeCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="FontSize" Storyboard.TargetName="copyrightShadow">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource LargeCopyrightFontSize}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <TextBlock
                x:Name="copyrightShadow" 
                VerticalAlignment="Bottom"
                HorizontalAlignment="Right"/>
            <TextBlock 
                x:Name="copyright" 
                Margin="0,0,1,1"
                VerticalAlignment="Bottom" 
                HorizontalAlignment="Right"/>
        </Grid>
    </ControlTemplate>

    <!-- SCALE -->
    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="ScalebarTemplate">
        <Grid 
            x:Name="LayoutRoot" 
            UseLayoutRounding="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="MapTypeStates">
                    <VisualState x:Name="Road">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="imperialTextShadow" To="White" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="imperialText" To="Black" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="metricTextShadow" To="White" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="metricText" To="Black" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="AerialBE">
                        <Storyboard>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="imperialTextShadow" To="Black" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="imperialText" To="White" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="metricTextShadow" To="Black" Duration="0"/>
                            <ColorAnimation Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="metricText" To="White" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <StackPanel 
                x:Name="scaleBarContainer" 
                Orientation="Horizontal">
                <StackPanel
                    Margin="0,0,2,0"
                    Orientation="Vertical">
                    <Grid>
                        <TextBlock
                            x:Name="imperialTextShadow" 
                            TextWrapping="Wrap" 
                            Text="250 miles" 
                            TextAlignment="Right" 
                            FontFamily="Arial" 
                            VerticalAlignment="Bottom" 
                            HorizontalAlignment="Right"/>
                        <TextBlock 
                            x:Name="imperialText" 
                            Margin="0,0,1,1" 
                            TextWrapping="Wrap" 
                            Text="250 miles" 
                            TextAlignment="Right" 
                            FontFamily="Arial" 
                            VerticalAlignment="Bottom" 
                            HorizontalAlignment="Right"/>
                    </Grid>
                    <Rectangle
                        x:Name="imperialScaleBar" 
                        Fill="#FFF4F4F5" 
                        Height="5" 
                        Stroke="#FF888888" 
                        VerticalAlignment="Bottom" 
                        Width="100" 
                        HorizontalAlignment="Right"/>
                </StackPanel>
                <StackPanel
                    Orientation="Vertical">
                    <Grid>
                        <TextBlock 
                            x:Name="metricTextShadow" 
                            TextWrapping="Wrap" 
                            Text="500 km" 
                            TextAlignment="Right" 
                            FontFamily="Arial" 
                            VerticalAlignment="Bottom" 
                            HorizontalAlignment="Right"/>
                        <TextBlock 
                            x:Name="metricText" 
                            Margin="0,0,1,1" 
                            TextWrapping="Wrap" 
                            Text="500 km" 
                            TextAlignment="Right" 
                            FontFamily="Arial" 
                            VerticalAlignment="Bottom" 
                            HorizontalAlignment="Right"/>
                    </Grid>
                    <Rectangle
                        x:Name="metricScaleBar" 
                        Fill="#FFF4F4F5" 
                        Height="5"  
                        Stroke="#FF888888" 
                        Width="100" 
                        HorizontalAlignment="Right"/>
                </StackPanel>
            </StackPanel>
        </Grid>
    </ControlTemplate>
    
    <ControlTemplate TargetType="bm:AccessibleContainer" x:Key="InvalidCredentialsMessageTemplate">
        <Grid x:Name="LayoutRoot" Background="White" Width="auto" Height="auto">
            <StackPanel Orientation="Horizontal" 
                x:Name="MessageStackPanel" 
                Width="auto" 
                Height="auto" 
                Background="White" 
                VerticalAlignment="Center" 
                HorizontalAlignment="Center">
                <TextBlock 
                    x:Name="MessageTextBlock" 
                    Text="Invalid Credentials. Sign up for a developer account at:" 
                    Foreground="Black" 
                    VerticalAlignment="Center" 
                    HorizontalAlignment="Center" 
                    TextWrapping="Wrap"
                    Padding="0"/>
                <HyperlinkButton 
                    x:Name="DeveloperPageHyperLink" 
                    Padding="0"
                    Content="http://www.microsoft.com/maps/developers" 
                    NavigateUri="http://www.microsoft.com/maps/developers"
                    VerticalAlignment="Center" 
                    HorizontalAlignment="Center" 
                    FontSize="{Binding ElementName=MessageTextBlock, Path=FontSize, Mode=OneWay}"/>
            </StackPanel>
        </Grid>
    </ControlTemplate>

